{
    "name": "Getting Started Example",
    "version": "0.0.3",
    "description": "Build an Extension!",
    "permissions": ["storage"],
    "manifest_version": 3,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "popup.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/fiat-fe-tool.png",
        "32": "/images/fiat-fe-tool.png",
        "48": "/images/fiat-fe-tool.png",
        "128": "/images/fiat-fe-tool.png"
      }
    },
    "icons": {
      "16": "/images/fiat-fe-tool.png",
      "32": "/images/fiat-fe-tool.png",
      "48": "/images/fiat-fe-tool.png",
      "128": "/images/fiat-fe-tool.png"
    },
    "options_page": "options.html"
  }